{
  "name": "event-notification-service",
  "version": "1.0.0",
  "description": "Event-driven notification system with Socket.io and BullMQ",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "worker": "nodemon src/workers/event.worker.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run worker\"",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "notification",
    "event-driven",
    "socket.io",
    "bullmq"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.66.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.2",
    "socket.io": "^4.8.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4"
  }
}
